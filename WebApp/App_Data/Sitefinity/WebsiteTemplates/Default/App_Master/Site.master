        <div id="page-top"></div>
        <div id="page">
        
            <div id="header" class="section" role="banner">
                <h2 id="site-name">
                    <a href="" rel="start" title="" tabindex="0"><span>Site name</span></a>
                    <small id="site-slogan">Site slogan</small>
                </h2>
                <div class="join-mailing-list"><a href="">Join Our Mailing List</a></div>
                <asp:ContentPlaceHolder ID="cphHeaderInfo" runat="server"/>
                <ul id="page-links" class="links invisible">
