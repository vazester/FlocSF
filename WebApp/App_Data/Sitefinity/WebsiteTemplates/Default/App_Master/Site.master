<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site.master.cs" Inherits="WebApp.Masterpages.Site" %>

<!DOCTYPE html>
<html>
<head runat="server">
    <title></title>
</head>
<body class="theme is-front">
    <form id="form1" runat="server">
